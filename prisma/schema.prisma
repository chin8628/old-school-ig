datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Photo {
  id           Int       @id @default(autoincrement())
  fileName     String?
  createdAt    DateTime? @default(now())
  shutterSpeed String?
  fNumber      String?
  maker        String?
  model        String?
  lensModel    String?
  createDate   DateTime?
  imageHeight  String?
  imageWidth   String?
  iso          String?
  focalLength  String?
  story        String?

  vibeSongId Int?
  vibeSong   VibeSong? @relation(fields: [vibeSongId], references: [id], onDelete: NoAction, onUpdate: NoAction)

  profileId Int?
  profile   Profile? @relation(fields: [profileId], references: [id])
}

model VibeSong {
  id        Int     @id @default(autoincrement())
  youtubeId String?
  startTime Int?
  endTime   Int?
  photo     Photo[]
}

model Profile {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  displayName String
  shortBio    String
  photo       Photo[]
}
